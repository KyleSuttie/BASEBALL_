<div id="player">
  <h1>MLB Player Stats. from 1998</h1>
    <p id="s">
      <body>
      <%= render(:partial => "select",
		 :object => @teams,
		 :object => params[:field],
		 :object => params[:dir],
		 :object => params[:page]) %>
      </body>
    </p>
    <table>
    <tbody id="f">
      <%= render(:partial => "field",
                 :collection => @fields,
                 :object => params[:page],
		 :object => params[:currentteam]) %>
    </tbody>
    
    <tbody id="p">
      <%= render(:partial => "player",
                 :collection => @players,
                 :object => params[:page],
                 :object => params[:num],
                 :object => params[:field]) %>
    </tbody>
  </table>
  <table id="pag">
    <%= render(:partial => "page",
               :object => params[:page]) %>
  </table>
  <table id="u">
    <%= render(:partial => "upload",
	       :object => params[:currentteam]) %>
  </table>
</div>
